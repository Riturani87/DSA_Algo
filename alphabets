/* A A A 
   B B B
   C C C */
   
PROGRAM  

#include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  while(i<=n){
  int j=1;
  while(j<=n){
      char ch=('A'+i-1);
      cout<<ch;
      j++;
      }
    cout<<endl;
   i++;
  }}   

/* A B C
   A B C
   A B C */
   
#include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  while(i<=n){
  int j=1;
  while(j<=n){
      char ch=('A'+j-1);
      cout<<ch;
      j++;
      }
    cout<<endl;
   i++;
  }}


/* A B C 
   D E F 
   G H I */
   
 #include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  char ch='A';
  while(i<=n){
  int j=1;
  while(j<=n){
      cout<<ch;
      ch= ch+1;
      j++;
      }
    cout<<endl;
   i++;
  }}
   
  /* A B C 
     B C D 
     C D E */
     
#include <iostream>
using namespace std;

  int main() {
  int n;
  cin>>n;
  int i=1;
  while(i<=n){
  int j=1;
  while(j<=n){
      char ch ='A'+(i+j-2);
      cout<<ch;
      j++;
      }
    cout<<endl;
   i++;
  }}
  
  
  /* A
     B B
     C C C */
     
   #include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  while(i<=n){
  int j=1;
  while(j<=i){
      char ch ='A'+(i-1);
      cout<<ch;
      j++;
      }
    cout<<endl;
   i++;
  }}  
  
  /* A
     B C
     D E F
     G H I J */
     
   #include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  char ch ='A';
  while(i<=n){
  int j=1;
  while(j<=n){
     cout<<ch;
      ch++;
      j++;
      }
    cout<<endl;
   i++;
  }}  
     
     
 /* A 
    B C 
    C D E
    D E F G */
    
    
 #include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
   
  while(i<=n){
  int j=1;
  while(j<=i){
     char ch ='A'+(i+j-2);
      cout<<ch;
      j++;
      }
    cout<<endl;
   i++;
  }}
  
  /* D
     C D
     B C D
     A B C D */
     
  #include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  
  while(i<=n){
  int j=1;
  char ch ='A'+(n-i);
  while(j<=i){
    
      cout<<ch;
       ch++;
      j++;
      }
    cout<<endl;
   i++;
  }}   
  
  
  
  
   
